{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "debug mee flow",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/src/start.ts",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "runtimeArgs": [
        "-r",
        "ts-node/register"
      ],
      "sourceMaps": true
    }
  ]
}